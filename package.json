{
  "name": "cem-plugin-complex-types",
  "version": "1.0.1-rc.1",
  "description": "This custom elements manifest analyzer plugin adds support for resolving complex types in custom element properties.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild index.ts --outfile=dist/index.js",
    "build:watch": "pnpm build --watch",
    "check": "pnpm check:script && pnpm check:format",
    "check:format": "prettier . --check",
    "check:format:fix": "prettier . --write",
    "check:lint": "eslint",
    "check:lint:fix": "eslint --fix"
  },
  "keywords": [
    "custom-elements",
    "custom-elements-json",
    "custom-elements-manifest",
    "customelements",
    "webcomponents",
    "customelementsjson",
    "customelementsmanifest"
  ],
  "author": "jls digital ag <https://jls.ch>",
  "license": "MIT",
  "dependencies": {
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.1.14",
    "@eslint/js": "^9.29.0",
    "@eslint/markdown": "^6.5.0",
    "custom-elements-manifest": "^2.1.0",
    "esbuild": "^0.25.6",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript": "^4.3.2"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
